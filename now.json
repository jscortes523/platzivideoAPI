{
    "name":"platzivideo",
    "version":2,
    "builds":[{"src":"src/index.js","use":"@now/node"}],
    "routes":[{"src":"src/(.*)", "dest":"/index.js"}],
    "env":{
        "DB_USER":"@platzivideodb-user",
        "DB_PASSWORD":"@platzivideodb-password",
        "DB_HOST":"@platzivideodb-host",
        "DB_NAME":"@platzivideodb-name"
    }
}